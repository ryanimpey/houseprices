generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by the Prisma Client.
model prices {
  Date                         String
  Region_Name                  String
  Area_Code                    String?
  Detached_Average_Price       Float?
  Detached_Index               Float?
  Detached_Monthly_Change      String?
  Detached_Annual_Change       String?
  Semi_Detached_Average_Price  Float?
  Semi_Detached_Index          Float?
  Semi_Detached_Monthly_Change String?
  Semi_Detached_Annual_Change  String?
  Terraced_Average_Price       Float?
  Terraced_Index               Float?
  Terraced_Monthly_Change      String?
  Terraced_Annual_Change       String?
  Flat_Average_Price           Float?
  Flat_Index                   Float?
  Flat_Monthly_Change          String?
  Flat_Annual_Change           String?

  @@id([Date, Region_Name])
}

model districts {
  OBJECTID     BigInt  @id
  LAD21CD      String?
  LAD21NM      String?
  LAD21NMW     String?
  BNG_E        BigInt?
  BNG_N        BigInt?
  LONG         Float?
  LAT          Float?
  GlobalID     String?
  SHAPE_Length Float?
  SHAPE_Area   Float?
}

model geojson {
  id       BigInt  @id @default(autoincrement())
  district String?
  long     Float?
  lat      Float?
  geometry Json?
}

model places {
  region   String  @id
  long     String?
  lat      String?
  geometry Json?
}
